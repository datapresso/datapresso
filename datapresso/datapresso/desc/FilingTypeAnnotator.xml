<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>com.dp.edgar.uima.annotator.FilingTypeAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>FilingTypeAnnotator</name>
    <description>A text analysis engine to detect the SEC filing tye based on the configured value.</description>
    <version/>
    <vendor>Datapresso</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>FilingType</name>
        <description>Contains a list of supported filing types.</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>FilingType</name>
        <value>
          <array>
            <string>DEF-14A</string>
            <string>10-K</string>
          </array>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="SourceDocInfo.xml"/>
        <import location="../../DataTool/desc/of_typesystem.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">uima.tcas.DocumentAnnotation</type>
          <type allAnnotatorFeatures="true">com.ibm.es.tt.DocumentMetaData</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">com.dp.edgar.uima.type.SourceDocInfo</type>
          <type allAnnotatorFeatures="true">com.ibm.es.tt.DocumentMetaData</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
